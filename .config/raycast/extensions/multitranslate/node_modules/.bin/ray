#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/@raycast+api@1.61.2_@types+node@16.18.61_@types+react@18.2.37/node_modules/@raycast/api/bin/node_modules:/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/@raycast+api@1.61.2_@types+node@16.18.61_@types+react@18.2.37/node_modules/@raycast/api/node_modules:/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/@raycast+api@1.61.2_@types+node@16.18.61_@types+react@18.2.37/node_modules/@raycast/node_modules:/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/@raycast+api@1.61.2_@types+node@16.18.61_@types+react@18.2.37/node_modules:/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/@raycast+api@1.61.2_@types+node@16.18.61_@types+react@18.2.37/node_modules/@raycast/api/bin/node_modules:/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/@raycast+api@1.61.2_@types+node@16.18.61_@types+react@18.2.37/node_modules/@raycast/api/node_modules:/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/@raycast+api@1.61.2_@types+node@16.18.61_@types+react@18.2.37/node_modules/@raycast/node_modules:/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/@raycast+api@1.61.2_@types+node@16.18.61_@types+react@18.2.37/node_modules:/Users/emile/WORKSPACE/github/dotfiles/.config/raycast/extensions/multitranslate/node_modules/.pnpm/node_modules:$NODE_PATH"
