if [ -x "$basedir//bin/bash" ]; then
  exec "$basedir//bin/bash"  "$basedir/../@raycast/api/bin/ray" "$@"
else
  exec /bin/bash  "$basedir/../@raycast/api/bin/ray" "$@"
fi
